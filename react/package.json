{
  "name": "@headless-toast/react",
  "version": "0.0.0",
  "type": "module",
  "files": [
    "dist",
    "src"
  ],
  "main": "./dist/bundles/@headless-toast/react.umd.cjs",
  "module": "./dist/bundles/@headless-toast/react.es.js",
  "exports": {
    ".": {
      "import": "./dist/bundles/@headless-toast/react.es.js",
      "require": "./dist/bundles/@headless-toast/react.umd.cjs"
    }
  },
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "dev": "concurrently --names \"JS,TYPES\" \"vite build --emptyOutDir false -w \" \"tsc --watch\"",
    "build": "concurrently --names \"JS,TYPES\" \"vite build\" \"tsc\"",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@types/node": "^18.14.6",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.10",
    "@types/uuid": "^9.0.1",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "@xstate/react": "^3.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^4.9.3",
    "uuid": "^9.0.0",
    "vite": "^4.1.0",
    "xstate": "^4.37.0"
  },
  "peerDependencies": {
    "react": "^18.x.x",
    "react-dom": "^18.x.x",
    "xstate": "^4.x.x",
    "uuid": "^9.x.x || ^8.x.x"
  }
}
